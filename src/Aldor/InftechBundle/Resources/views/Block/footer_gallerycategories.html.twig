<div class="widget">
<h4 class="widget-title">{{title}}</h4>

    <ul class="entries links links-2-cols">
    {% for category in categories%}
        <li>
        <a href="{{ path('gallery_photocategory', {'galleryslug': category.gallery.slug, 'photocategoryslug': category.slug }) }}">
            {{ category.name }}</a>
        </li>
    {% endfor %}
    </ul>
</div>

